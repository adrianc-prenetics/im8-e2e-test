{
  "name": "im8-e2e-test",
  "version": "1.0.0",
  "description": "Cypress E2E testing suite for im8store.myshopify.com",
  "main": "index.js",
  "scripts": {
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:critical": "cypress run --spec 'cypress/e2e/critical/**/*.cy.js'",
    "cy:run:desktop": "cypress run --config viewportWidth=1280,viewportHeight=720",
    "cy:run:mobile": "cypress run --config viewportWidth=375,viewportHeight=812",
    "cy:run:headed": "cypress run --headed",
    "cy:run:chrome": "cypress run --browser chrome",
    "cy:run:firefox": "cypress run --browser firefox",
    "test": "npm run cy:run",
    "test:ci": "cypress run --record false"
  },
  "keywords": [
    "cypress",
    "e2e",
    "testing",
    "shopify",
    "im8"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "cypress": "^13.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:adrianc-prenetics/im8-e2e-test.git"
  }
}
