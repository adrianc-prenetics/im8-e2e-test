{
  "name": "im8-e2e-test",
  "version": "2.0.0",
  "description": "Playwright E2E testing suite for im8health.com",
  "main": "index.js",
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:chromium": "playwright test --project=chromium",
    "test:mobile": "playwright test --project=mobile",
    "report": "playwright show-report",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:critical": "cypress run --spec 'cypress/e2e/critical/**/*.cy.js'"
  },
  "keywords": [
    "playwright",
    "e2e",
    "testing",
    "shopify",
    "im8"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "cypress": "^13.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:adrianc-prenetics/im8-e2e-test.git"
  }
}
